{
  "name": "tianfuunion/mark-auth",
  "description": "The Tianfu Union Mark Engine Authorize.",
  "keywords": [
    "tianfuunion",
    "mark-auth"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://auth.tianfu.ink",
  "authors": [
    {
      "name": "Mark Yount",
      "email": "mark@tianfuunion.cn"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "tianfuunion/mark-engine": "^1.0 || dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0|~8.0",
    "php-coveralls/php-coveralls": "^2.2",
    "phpstan/phpstan": "^0.12"
  },
  "autoload": {
    "psr-4": {
      "mark\\auth\\": "src/auth"
    },
    "psr-0": {
      "": ""
    },
    "files": [
      ""
    ]
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://mirrors.aliyun.com/composer/"
    }
  }
}